    if (Test-Path "$(${env:ProgramFiles(x86)})\Microsoft OneDrive\OneDrive.exe") {
        $FileInfo = Get-Item -Path "${env:ProgramFiles(x86)})\Microsoft OneDrive\OneDrive.exe"
        $NewObj = [PSCustomObject]@{
            FilePath = $FileInfo.FullName
            Version  = $FileInfo.VersionInfo.ProductVersion
        }
        $OneDriveVersions.Add($NewObj)
    }

    foreach ($Path in (Get-UsersToEval).LocalPath) {
        if (Test-Path "$($Path)\AppData\Local\Microsoft\OneDrive\OneDrive.exe") {
            $FileInfo = Get-Item -Path "$($Path)\AppData\Local\Microsoft\OneDrive\OneDrive.exe"
