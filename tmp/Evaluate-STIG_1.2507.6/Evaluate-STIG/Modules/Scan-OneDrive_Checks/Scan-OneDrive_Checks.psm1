    #---=== Begin Custom Code ===---#
    $Compliant = $true
    $OneDriveVersions = [System.Collections.Generic.List[System.Object]]::new()

    if (Test-Path "$env:ProgramFiles\Microsoft OneDrive\OneDrive.exe") {
        $FileInfo = Get-Item -Path "$env:ProgramFiles\Microsoft OneDrive\OneDrive.exe"
        $NewObj = [PSCustomObject]@{
            FilePath = $FileInfo.FullName
            Version  = $FileInfo.VersionInfo.ProductVersion
        }
        $OneDriveVersions.Add($NewObj)
