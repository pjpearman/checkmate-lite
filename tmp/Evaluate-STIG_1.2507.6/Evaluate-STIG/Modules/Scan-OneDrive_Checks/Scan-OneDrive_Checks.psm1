                    $FileInfo = Get-Item -Path $ExecutablePath
                    $NewObj = [PSCustomObject]@{
                        FilePath = $FileInfo.FullName
                        Version  = $FileInfo.VersionInfo.ProductVersion
                    }
                    $OneDriveVersions.Add($NewObj)
                }
            }
        }
    }

    if (($OneDriveVersions | Measure-Object).Count -gt 0) {
        $FindingDetails += "OneDrive Versions:`r`n" | Out-String
        foreach ($Item in $OneDriveVersions) {
            $FindingDetails += "Path:`t`t$($Item.FilePath)" | Out-String
            if ([Version]$Item.Version -gt [Version]"24.0") {
                $FindingDetails += "Version:`t$($Item.Version)" | Out-String
            }
            else {
                $Compliant = $false
                $FindingDetails += "Version:`t$($Item.Version) [finding]" | Out-String
            }
