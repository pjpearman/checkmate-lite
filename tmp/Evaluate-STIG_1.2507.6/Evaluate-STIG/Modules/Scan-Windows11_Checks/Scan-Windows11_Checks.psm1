            If (($FileFindings | Measure-Object).Count -gt 0) {
                $FindingDetails += "Certificate .p12 and/or .pfx files:" | Out-String
                $FindingDetails += "---------------------" | Out-String
                ForEach ($File in $FileFindings) {
                    $FindingDetails += "File:`t`t$(Split-Path $File -Leaf)" | Out-String
                    $FindingDetails += "Path:`t`t$(Split-Path $File -Parent)" | Out-String
                    $FindingDetails += "Created:`t$((Get-ChildItem $File -Force).CreationTime)" | Out-String
