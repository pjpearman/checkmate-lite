    $DomainRole = Get-DomainRoleStatus
    If (-Not($DomainRole.RoleFriendlyName -in @("Backup Domain Controller", "Primary Domain Controller"))) {
        $Status = "Not_Applicable"
        $FindingDetails += "System is a '$($DomainRole.RoleFriendlyName)' so this requirement is NA." | Out-String
    }
    Else {
        $ValidUPNs = New-Object System.Collections.Generic.List[System.Object]
        $LocalDomain = (Get-ADDomain).DnsRoot
        $LocalCert = Get-ADDomainControllerCertificate -ComputerName $env:COMPUTERNAME | Where-Object {($_.EnhancedKeyUsageList.FriendlyName -contains "KDC Authentication")}
        If ($LocalCert) {
            $ApprovedChain = $false
            # Build the cert chain
            $Chain = [System.Security.Cryptography.X509Certificates.X509Chain]::new()
            $null = $Chain.Build($LocalCert)
            ForEach ($Cert in ($Chain.ChainElements.Certificate | Select-Object FriendlyName,Subject)) {
                If ($Cert.Subject -match "(^CN=DOD Root|^CN=ECA Root|^CN=NSS Root)") {
                    $ApprovedChain = $true
                }
            }
            If ($ApprovedChain -eq $true) {
                $NewObj = [PSCustomObject]@{
                    Suffix      = $LocalDomain
                    CertSubject = $LocalCert.Subject
                    CertIssuer  = $LocalCert.Issuer
                    Thumbprint  = $LocalCert.Thumbprint
                }
                $ValidUPNs.Add($NewObj)
            }
        }

        $AltUPNs = Get-ADForest | Select-Object -ExpandProperty UPNSuffixes
        ForEach ($Item in $AltUPNs) {
            If ($Item -in @("mil", "smil.mil")) {
                $NewObj = [PSCustomObject]@{
                    Suffix      = $Item
                    CertSubject = "Well Known UPN suffix"
                    CertIssuer  = "Well Known UPN suffix"
                    Thumbprint  = "Well Known UPN suffix"
                }
                $ValidUPNs.Add($NewObj)
            }
            ElseIf ($Item -in (Get-ADTrust -Filter *).Target) {
                $RemoteCert = Get-ADDomainControllerCertificate -Domain $Item | Where-Object {($_.EnhancedKeyUsageList.FriendlyName -contains "KDC Authentication")}
                If ($RemoteCert) {
                    $ApprovedChain = $false
                    # Build the cert chain
                    $Chain = [System.Security.Cryptography.X509Certificates.X509Chain]::new()
                    $null = $Chain.Build($RemoteCert)
                    ForEach ($Cert in ($Chain.ChainElements.Certificate | Select-Object FriendlyName,Subject)) {
                        If ($Cert.Subject -match "(^CN=DOD Root|^CN=ECA Root|^CN=NSS Root)") {
                            $ApprovedChain = $true
                        }
                    }
                    If ($ApprovedChain -eq $true) {
                        $NewObj = [PSCustomObject]@{
                            Suffix      = $Item
                            CertSubject = $RemoteCert.Subject
                            CertIssuer  = $RemoteCert.Issuer
                            Thumbprint  = $RemoteCert.Thumbprint
                        }
                        $ValidUPNs.Add($NewObj)
                    }
                }
            }
        }

        ForEach ($Item in $ValidUPNs) {
            $FindingDetails += "Trusted Suffix:`t`t`t$($Item.Suffix)" | Out-String
            $FindingDetails += "DC Cert Subject:`t`t$($Item.CertSubject)" | Out-String
            $FindingDetails += "DC Cert Issuer:`t`t`t$($Item.CertIssuer)" | Out-String
            $FindingDetails += "DC Cert Thumbprint:`t$($Item.Thumbprint)" | Out-String
            $FindingDetails += "" | Out-String
        }

        $AccountsToVerify = Get-ADUser -Filter {Name -ne "krbtgt" -and Enabled -eq $true} | Where-Object {($_.Name -notmatch '\$$' -and ($_.UserPrincipalName -split "@")[1] -notin (Get-UpnSuffixLevels -UpnList $ValidUPNs.Suffix))}
        If ($AccountsToVerify) {
            $Status = "Open"
            $FindingDetails += "There are $(($AccountsToVerify | Measure-Object).Count) account findings:" | Out-String
            $FindingDetails += "-------------------------------------" | Out-String
            ForEach ($Account in $AccountsToVerify) {
                $FindingDetails += "Name:`t$($Account.Name)" | Out-String
                $FindingDetails += "Enabled:`t$($Account.Enabled)" | Out-String
                $FindingDetails += "UPN:`t$($Account.UserPrincipalName)" | Out-String
                $FindingDetails += "DN:`t`t$($Account.DistinguishedName)" | Out-String
                $FindingDetails += "" | Out-String
                If (($FindingDetails | Measure-Object -Character).Characters -ge 38000) {
                    Break
                }
            }
        }
        Else {
            $Status = "NotAFinding"
            $FindingDetails += "No accounts found with an inappropriate UPN." | Out-String
        }
    }
    #---=== End Custom Code ===---#

    if ($FindingDetails.Trim().Length -gt 0) {
        $ResultHash = Get-TextHash -Text $FindingDetails -Algorithm SHA1
    }
    else {
        $ResultHash = ""
    }

    if ($PSBoundParameters.AnswerFile) {
        $GetCorpParams = @{
            AnswerFile   = $PSBoundParameters.AnswerFile
            VulnID       = $VulnID
            RuleID       = $RuleID
            AnswerKey    = $PSBoundParameters.AnswerKey
            Status       = $Status
            Hostname     = $Hostname
            Username     = $Username
            UserSID      = $UserSID
            Instance     = $Instance
            Database     = $Database
            Site         = $Site
            ResultHash   = $ResultHash
            ResultData   = $FindingDetails
            ESPath       = $ESPath
            LogPath      = $LogPath
            LogComponent = $LogComponent
            OSPlatform   = $OSPlatform
        }

        $AnswerData = (Get-CorporateComment @GetCorpParams)
        if ($Status -eq $AnswerData.ExpectedStatus) {
            $AFKey = $AnswerData.AFKey
            $AFStatus = $AnswerData.AFStatus
            $Comments = $AnswerData.AFComment | Out-String
