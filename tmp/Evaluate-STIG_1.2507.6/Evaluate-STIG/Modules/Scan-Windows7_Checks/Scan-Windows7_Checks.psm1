                            }
                            Else {
                                $FindingDetails += "Value:`t`t$($RegistryResultValue) [Expected $($Item.RegistryValue) (or less)]" | Out-String
                            }
                            If ($RegistryResult.Type -eq $Item.RegistryType) {
