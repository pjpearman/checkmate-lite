            Catch {
                $Audit = "Failed - $($_.Exception.Message)"
            }
            $NewObj = [PSCustomObject]@{
                DisplayName = $GPO.DisplayName
                Id          = $GPO.Id
                Auditing    = $Audit
            }
            $ObjectList.Add($NewObj)
        }

        If ($ObjectList.Auditing -like "Failed - *") {
            ForEach ($Object in $ObjectList | Where-Object Auditing -like "Failed - *") {
