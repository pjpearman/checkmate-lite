    If ($DomainRole.RoleFriendlyName -notin @("Backup Domain Controller", "Primary Domain Controller")) {
        $Status = "Not_Applicable"
        $FindingDetails += "System is a '$($DomainRole.RoleFriendlyName)' so this requirement is NA." | Out-String
    }
    Else {
        $TempUserHivePath = ""  # User's loaded hive to perform check
        $RegistryPath = "HKLM:\SOFTWARE\Microsoft\Windows\CurrentVersion\Policies\System\KDC\Parameters\"  # Registry path identified in STIG
