    $Justification = ""  # If SeverityOverride is used, a justification is required.
    # $ResultObject = [System.Collections.Generic.List[System.Object]]::new()

    #---=== Begin Custom Code ===---#
    $DomainRole = Get-DomainRoleStatus
    If (-Not($DomainRole.RoleFriendlyName -in @("Backup Domain Controller", "Primary Domain Controller"))) {
        $Status = "Not_Applicable"
        $FindingDetails += "System is a '$($DomainRole.RoleFriendlyName)' so this requirement is NA." | Out-String
    }
    Else {
        $ObjectList = New-Object System.Collections.Generic.List[System.Object]
        If (($PsVersionTable.PSVersion -join ".") -lt [Version]"6.0") {
            Import-Module ActiveDirectory
            $GPOs = Get-GPO -All -Server $env:COMPUTERNAME
        }
        Else {
            $GPOs = Invoke-Expression "PowerShell.exe -NoProfile -Command {Import-Module ActiveDirectory; Get-GPO -All -Server $env:COMPUTERNAME}"
        }
        ForEach ($GPO in $GPOs) {
            Try {
                $Audit = (Get-Acl -Path "AD:$($GPO.Path)" -Audit -ErrorAction Stop).Audit | Sort-Object -Unique IdentityReference, ActiveDirectoryRights | Select-Object AuditFlags, IdentityReference, ActiveDirectoryRights, IsInherited, InheritanceType
                If (-Not($Audit)) {
                    $Audit = "No Auditing"
                }
