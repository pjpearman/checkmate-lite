                            Name          = $Account.Name
                            SID           = $Account.SID
                            Enabled       = "$($Account.Enabled) [Finding]"
                            Type          = $Account.ObjectClass
                            Administrator = $Administrator
                        }
                        $NonCompliantAccounts.Add($NewObj)
                    }
