        [String]$Database,

        [Parameter(Mandatory = $false)]
        [String]$SiteName
    )

    $ModuleName = (Get-Command $MyInvocation.MyCommand).Source
    $FuncDescription = ($MyInvocation.MyCommand.ScriptBlock -split "#>")[0].split("`r`n")
    $VulnID = ($FuncDescription | Select-String -Pattern "V-\d{4,6}$").Matches[0].Value
    $RuleID = ($FuncDescription | Select-String -Pattern "SV-\d{4,6}r\d{1,}_rule$").Matches[0].Value
    $Status = "Not_Reviewed"  # Acceptable values are 'Not_Reviewed', 'Open', 'NotAFinding', 'Not_Applicable'
    $FindingDetails = ""
    $Comments = ""
    $AFStatus = ""
    $SeverityOverride = ""  # Acceptable values are 'CAT_I', 'CAT_II', 'CAT_III'.  Only use if STIG calls for a severity change based on specified critera.
    $Justification = ""  # If SeverityOverride is used, a justification is required.
    # $ResultObject = [System.Collections.Generic.List[System.Object]]::new()

    #---=== Begin Custom Code ===---#
    If (-Not(Test-Path -Path $env:windir\Temp\Evaluate-STIG\Evaluate-STIG_FilesToScan.txt)) {
        $FindingDetails += "*** $env:windir\Temp\Evaluate-STIG\Evaluate-STIG_FilesToScan.txt does not exist so unable to complete this check.  Consider increasing the -FileSearchTimeout parameter. ***" | Out-String
    }
    Else {
        $FileFindings = [System.Collections.Generic.List[System.Object]]::new()
        $NonCertFindings = [System.Collections.Generic.List[System.Object]]::new()
        $Files = Get-Content $env:windir\Temp\Evaluate-STIG\Evaluate-STIG_FilesToScan.txt | Select-String -Pattern "(\.p12$|\.pfx$)"
        If ($Files) {
            ForEach ($File in $Files) {
                If (-Not($((Split-Path $File -Leaf) -eq "Preflight.p12") -and (Test-Path -Path $(Join-Path -Path $(Split-Path $File -Parent) -ChildPath "PreflightLib.dll")))) {
                    Try {
                        Get-PfxData -FilePath $File -ErrorAction Stop | Out-Null
                        $FileFindings.Add($File)
                    }
                    Catch {
                        If ($_.Exception.Message -like "*ERROR_INVALID_PASSWORD*") {
                            $FileFindings.Add($File)
                        }
                        Else {
                            $NonCertFindings.Add($File)
                        }
                    }
                }
            }
        }

        If (($FileFindings | Measure-Object).Count -eq 0 -and ($NonCertFindings | Measure-Object).Count -eq 0) {
            $Status = "NotAFinding"
            $FindingDetails += "No .p12 or .pfx certificate files found." | Out-String
        }
        Else {
            If (($FileFindings | Measure-Object).Count -gt 0) {
                $FindingDetails += "Certificate .p12 and/or .pfx files:" | Out-String
                $FindingDetails += "---------------------" | Out-String
                ForEach ($File in $FileFindings) {
                    $FindingDetails += "File:`t`t$(Split-Path $File -Leaf)" | Out-String
                    $FindingDetails += "Path:`t`t$(Split-Path $File -Parent)" | Out-String
                    $FindingDetails += "Created:`t$((Get-ChildItem $File -Force).CreationTime)" | Out-String
                    $FindingDetails += "" | Out-String
                }
            }
            Else {
                $Status = "NotAFinding"
            }

