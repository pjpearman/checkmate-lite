            }
            Else {
                $Compliant = $false
                $FindingDetails += "Configured:`t$($Item.Configured) [finding]" | Out-String
            }
            $FindingDetails += "" | Out-String
        }

        If ($Compliant -eq $true) {
            $Status = "NotAFinding"
        }
        Else {
