                    $NewObj = [PSCustomObject]@{
                        Name          = $Account.Name
                        SID           = $Account.SID
                        Enabled       = $Account.Enabled
                        Type          = $Account.ObjectClass
                        Administrator = $Administrator
                    }
                    $CompliantAccounts.Add($NewObj)
                }
                Else {
                    $NewObj = [PSCustomObject]@{
                        Name          = $Account.Name
                        SID           = $Account.SID
                        Enabled       = $Account.Enabled
